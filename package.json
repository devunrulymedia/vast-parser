{
  "name": "vast-parser",
  "version": "1.1.0",
  "description": "Parses VAST files into plain javascript objects",
  "main": "src/js/index.js",
  "scripts": {
    "start": "python -m SimpleHTTPServer",
    "lint": "./node_modules/.bin/eslint src/* && ./node_modules/.bin/eslint -c test/.eslintrc test/*",
    "test": "mocha --recursive --reporter list test/mocha.conf.js ./test",
    "testWatch": "npm run lint && npm run testMocha -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/devunrulymedia/vast-parser.git"
  },
  "keywords": [
    "vast"
  ],
  "author": "marmalade@unrulymedia.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devunrulymedia/vast-parser/issues"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.1.1",
    "jsdom": "^11.3.0",
    "mocha": "^2.5.3",
    "node-fetch": "^1.7.3",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "squirejs": "^0.2.1",
    "url": "^0.11.0",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "dom-parser": "^0.1.5",
    "es6-promise": "^3.2.1",
    "jquery": "^1.11.2"
  }
}
